fn count(n)
{
	if((n & 1) == 0)
	{
		return 0;
	}

	return 1;
}

fn fib(n)
{
	if(n < 2) { return n; }
	return fib(n - 2) + fib(n - 1);
}

fn iter()
{
	int i = 32;
	for(int x = 0; x < 20; ++x)
	{
		for(int y = 0; y < 20; ++y)
		{
			pchr(i);
			++i;
			if(i == 126)
			{
				i = 32;
			}
		}

		pchr('\n');
	}
}

fn main()
{
	int i = 0, cnt = 0;

	iter();

	while(i <= 100)
	{
		if(count(i))
		{
			++cnt;
		}

		++i;
	}
	test(cnt);
	test(fib(30));
}
